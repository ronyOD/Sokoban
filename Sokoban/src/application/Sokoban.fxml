<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="370.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SokobanController">
	<children>
		<MenuBar VBox.vgrow="ALWAYS">
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem fx:id="load" mnemonicParsing="false" text="Load" />
						<MenuItem fx:id="save" mnemonicParsing="false" text="Save" />
						<MenuItem fx:id="results" mnemonicParsing="false" text="Results" />
						<MenuItem fx:id="solution" mnemonicParsing="false" text="Solution" />
						<MenuItem fx:id="exit" mnemonicParsing="false" text="Exit" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Pane fx:id="level" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
		</AnchorPane>
	</children>
</VBox>
